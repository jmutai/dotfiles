let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
argglobal
let s:cpo_save=&cpo
set cpo&vim
nmap <buffer> \q <Plug>MailQuote
vmap <buffer> \q <Plug>MailQuote
nnoremap <buffer> <Plug>MailQuote :.,$s/^/> /:noh``
vnoremap <buffer> <Plug>MailQuote :s/^/> /:noh``
let &cpo=s:cpo_save
unlet s:cpo_save
setlocal keymap=
setlocal noarabic
setlocal autoindent
setlocal backupcopy=
setlocal balloonexpr=
setlocal nobinary
setlocal nobreakindent
